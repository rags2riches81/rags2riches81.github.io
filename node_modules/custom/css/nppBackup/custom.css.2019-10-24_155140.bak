#header {
  z-index          : 10;
  width            : 100%;
  height           : 110vh;
  min-height       : 110vh !important;
  padding-top      : 30vh;
  color            : #37dbc9;
  background-color : rgba(24, 21, 21, .77);
  vertical-align   : middle;
  animation        : surprise 20s linear 0s;
  /* [disabled]height: 100vh; */
}

#reveal {
/*  position         : relative;*/
  top              : 0;
  left             : 0;
  width            : 100%;
  height           : 100vh;
  min-height       : 100vh!important;
  margin           : 0 0;
  padding-top      : 20vh!important;
  background-image : url('../../../img/ABD_Spring2016.jpg');
  background-size  : cover;
  vertical-align   : middle;
  animation        : see 1.5s linear 0s!important;
  /* z-index: 10; */
}

#surprise {
  width            : 100%;
  height           : 100vh;
  min-height       : 100vh !important;
  margin           : 0 0;
  padding          : 0 0;
  background-image : url('../../../img/1.jpg');
  text-align       : right;
  vertical-align   : middle;
  /* background-color: #082C38; */
}

.A {
  text-align:right;
  /*display   : inline;*/
  opacity   : 0;
  font-size : 4rem;
  animation : see2 1.5s ease-out 1.5s forwards;
  /* text-shadow: .125rem .125rem #F9F4F4; */
}

.B {
  display   : inline;
  color     : #37dbc9;
  opacity   : 0;
  animation : see2 1.5s linear 3s forwards;
}

.D {
  display     : inline;
  color       : #faf5f5;
  opacity     : 0;
  text-shadow : .125rem .125rem #37dbc9;
  animation   : see 1.5s linear 4.5s forwards;
}

.initial {
  display       : inline;
  margin-right  : 0;
  padding-right : 0;
  opacity       : 0;
  text-shadow   : .125rem .125rem #f9f4f4;
  animation     : see 1.5s linear 1.5s forwards;
}

.fullname {
  display     : inline;
  color       : #faf5f5;
  opacity     : 0;
  text-shadow : .125rem .125rem #37dbc9;

  animation   : see 1.5s linear 3s forwards;
}

#mission {
  color       : #ff550b!important;
  opacity     : 0;
  text-shadow : 0 0 .7rem black;
  text-shadow :0 0 .25rem #111;
  /*text-shadow : 2px 3px 0.5rem #248cca;*/
