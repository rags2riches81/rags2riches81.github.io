@keyframes see {
  0% {
    opacity : 0;
  }

  25% {
    opacity : 0;
  }

  50% {
    opacity     : .30;
    /*text-shadow : 0 0 #37dbc9;*/
    text-shadow : 0 0 #111;
  }

  75% {
    opacity     : .65;
    text-shadow : .0625rem .0625rem .0rem #111;
  /*  text-shadow : .125rem .125rem .0625rem #37dbc9;*/
  }

  100% {
    opacity     : 1.0;
    text-shadow : .125rem .125rem .0625rem #111;
  /*  text-shadow : .25rem .25rem .125rem #37dbc9;*/
  }
}

@keyframes see2 {
  0% {
    opacity : 0;
  }

  25% {
    opacity : 0;
  }

  50% {
    opacity     : .30;
    text-shadow : 0 0 #ff550b;
    text-shadow : 0 0 #111;
  }

  75% {
    opacity     : .65;
    text-shadow : .0625rem .0625rem .0rem #111;
    text-shadow : .125rem .125rem .0625rem #ff550b;
  }

  100% {
    opacity     : 1.0;

    text-shadow : .25rem .25rem .0625rem #ff550b;
    text-shadow : .185rem .185rem .25rem #111;  }
}

@keyframes unsee {
  0% {
    opacity : 1.0;
  }

  25% {
    opacity : 1.0;
  }

  50% {
    opacity : 1.0;
  }

  75% {
    opacity : .70;
  }

  100% {
    opacity : .0;
  }
}

@keyframes surprise {
  0% {
    background-color : rgba(24, 21, 21, .77);
  }

  50% {
    background-color : rgba(24, 21, 21, .77);
  }

  60% {
    background-color : rgba(24, 21, 21, .52);
  }

  70% {
    background-color : rgba(24, 21, 21, .42);
  }

  80% {
    background-color : rgba(24, 21, 21, .32);
  }

  85% {
    background-color : rgba(24, 21, 21, .22);
  }

  90% {
    background-color : rgba(24, 21, 21, .17);
  }

  95% {
    background-color : rgba(24, 21, 21, .12);
  }

  100% {
    background-color : rgba(24, 21, 21, .07);
  }
}

